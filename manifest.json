{
    "manifest_version": 2,

    "name": "Kogan Chrome Extension",
    "description": "Kogan Product Notifications",
    "version": "1.0",

    "permissions": [
        "http://www.kogan.com/",
        "https://www.googleapis.com/",
        "pushMessaging",
        "notifications",
        "background"
    ],
    "background": {
        "scripts": ["js/background.js"]
    },
    "web_accessible_resources": [
        "icon.png"
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}
